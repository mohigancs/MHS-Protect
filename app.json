{
  "expo": {
    "name": "SchoolProtect",
    "slug": "SchoolProtect",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/MHS_Logo_Square.png",
    "splash": {
      "image": "./src/images/MHS_Logo_Square.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.MohiganCS.SchoolProtect",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.MohiganCS.SchoolProtect",
      "versionCode": 1,
      "config": {
        "googleMaps": { "apiKey": "askAmandaForKeyorFindItYourself" }   
      }
    },
    "notification": {
      "icon": "./src/images/notificationMailbox.png",
      "color": "#c80d00"
    }

  }
}
